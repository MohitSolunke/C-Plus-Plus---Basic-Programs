//Record BreKER

#include<iostream>
using namespace std;

int main()
{
    int n;
    cin>>n;

    int arr[n];

    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int recordb =0;
    for(int i=0;i<n;i++){
    if(arr[i]>arr[i-1] && arr[i]>arr[i+1])
    {
        cout<<arr[i]<<" ";
        recordb++;
    }
    }
    cout<<endl;
    cout<<recordb<<endl;
    return 0;
}
